    "paths": {
        "/api/v1/docker/images/inspect": {
            "get": {
                "parameters": [
                    {
                        "in": "query",
                        "name": "image_name",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "summary": "Inspect a Docker image and returns the full JSON response as is from the Docker APIs at /images/{image_name}/json expects image_name as a query parameter.",
                "description": "",
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "nullable": true,
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/DockerInspect"
                                        }
                                    ]
                                }
                            }
                        },
                        "description": ""
                    },
                    "204": {
                        "description": "Image not found"
                    }
                }
            }
        }
    },
